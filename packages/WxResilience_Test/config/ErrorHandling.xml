<errorHandling xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="errorHandling.xsd">
	<location name="wx">
		<location name="resilienceTest">
			<location name="pub">	 
                 <location name="test_handleError">
                     <exception type="all" errorToBeThrown="FATAL" errorHandlingId="WxResilienceTest.top.003" printBusinessObject="true">
                         <handling>wx.resilience.pub.resilience.errorHandling:logError</handling>
                     </exception>
                 </location>
                 <location name="test_handleErrorNestedService">
                     <exception type="all" errorToBeThrown="NONE" errorMessageContains="Error" errorHandlingId="WxResilienceTest.top.001" printBusinessObject="true">
                         <handling>wx.resilience.pub.resilience.errorHandling:logError</handling>
                     </exception>
                 </location>
                 <location name="subService">
                     <exception type="all" errorToBeThrown="FATAL" errorMessageRegex=".*rr.*" errorHandlingId="WxResilienceTest.top.002" printBusinessObject="true">
                         <handling>wx.resilience.pub.resilience.errorHandling:logError</handling>
                     </exception>
                 </location>
			</location>
		</location>
	</location>
</errorHandling>